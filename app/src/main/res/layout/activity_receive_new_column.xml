<?xml version="1.0" encoding="utf-8"?>

<TableLayout android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="0,1"
    android:shrinkColumns="*"
    android:weightSum="1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <TableRow
        android:id="@+id/tableRow0"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ColID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."
            android:layout_weight="0.1"/>


    </TableRow>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ColDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."
            android:layout_weight="1"/>

    </TableRow>
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">


        <TextView
            android:id="@+id/ColRef"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."
            android:layout_weight="1"/>



    </TableRow>
    <TableRow
        android:id="@+id/tableRow12"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ColSource"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."

            android:layout_weight="1"/>





    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ColDest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."

            android:layout_weight="1"/>





    </TableRow>
    <TableRow
        android:id="@+id/tableRow22"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ColRem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:text="LT."

            android:layout_weight="1"/>





    </TableRow>
    <TableRow
        android:id="@+id/tableRow21"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/CmdReceive"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_weight="1"
            />




    </TableRow>

    <TableRow
        android:id="@+id/tableRowMap1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="3">



        <Button
            android:id="@+id/CmdMapR"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"

            />
        <Button
            android:id="@+id/CmdMapD"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"

            />

        <Button
            android:id="@+id/CmdMapRoute"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"

            />

    </TableRow>




</TableLayout>